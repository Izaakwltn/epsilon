{
  "tags": [
    "scope:epsilon",
    "type:library",
    "platform:darwin"
  ],
  "targets": {
    "build": {
      "options": {
        "cwd": ".",
        "command": "if [ \"$(uname -s)\" != \"Darwin\" ]; then echo 'Skipping: wrong platform (need darwin)'; exit 0; fi && ./epsilon/epsilon --module epsilon.darwin --eval t"
      },
      "inputs": [
        "{projectRoot}/**/*.lisp",
        "{projectRoot}/module.lisp"
      ],
      "executor": "nx:run-commands",
      "cache": true
    },
    "test": {
      "options": {
        "cwd": ".",
        "command": "if [ \"$(uname -s)\" != \"Darwin\" ]; then echo 'Skipping: wrong platform (need darwin)'; exit 0; fi && ./epsilon/epsilon --test epsilon.darwin"
      },
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "{projectRoot}/**/*.lisp"
      ],
      "executor": "nx:run-commands",
      "cache": true
    }
  },
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "epsilon-darwin",
  "sourceRoot": "epsilon/modules/darwin",
  "implicitDependencies": [
    "epsilon-foreign"
  ],
  "projectType": "library"
}
