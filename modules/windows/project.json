{
  "tags": [
    "scope:epsilon",
    "type:library",
    "platform:windows"
  ],
  "targets": {
    "build": {
      "options": {
        "cwd": ".",
        "command": "if [ \"$(uname -s)\" != \"Windows\" ]; then echo 'Skipping: wrong platform (need windows)'; exit 0; fi && ./epsilon --module epsilon.windows --eval t"
      },
      "inputs": [
        "{projectRoot}/**/*.lisp",
        "{projectRoot}/module.lisp"
      ],
      "executor": "nx:run-commands",
      "cache": true
    },
    "test": {
      "options": {
        "cwd": ".",
        "command": "if [ \"$(uname -s)\" != \"Windows\" ]; then echo 'Skipping: wrong platform (need windows)'; exit 0; fi && ./epsilon --test epsilon.windows"
      },
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "{projectRoot}/**/*.lisp"
      ],
      "executor": "nx:run-commands",
      "cache": true
    }
  },
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "epsilon-windows",
  "sourceRoot": "epsilon/modules/windows",
  "implicitDependencies": null,
  "projectType": "library"
}
