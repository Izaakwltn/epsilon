{
  "tags": [
    "scope:epsilon",
    "type:library"
  ],
  "targets": {
    "build": {
      "options": {
        "cwd": ".",
        "command": "./epsilon/epsilon --module epsilon.release --eval t"
      },
      "inputs": [
        "{projectRoot}/**/*.lisp",
        "{projectRoot}/module.lisp"
      ],
      "executor": "nx:run-commands",
      "cache": true
    },
    "test": {
      "options": {
        "cwd": ".",
        "command": "./epsilon/epsilon --test epsilon.release"
      },
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "{projectRoot}/**/*.lisp"
      ],
      "executor": "nx:run-commands",
      "cache": true
    }
  },
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "epsilon-release",
  "sourceRoot": "epsilon/modules/release",
  "implicitDependencies": [
    "epsilon-digest",
    "epsilon-test"
  ],
  "projectType": "library"
}
