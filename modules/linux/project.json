{
  "tags": [
    "scope:epsilon",
    "type:library",
    "platform:linux"
  ],
  "targets": {
    "build": {
      "options": {
        "cwd": ".",
        "command": "if [ \"$(uname -s)\" != \"Linux\" ]; then echo 'Skipping: wrong platform (need linux)'; exit 0; fi && ./epsilon --module epsilon.linux --eval t"
      },
      "inputs": [
        "{projectRoot}/**/*.lisp",
        "{projectRoot}/module.lisp"
      ],
      "executor": "nx:run-commands",
      "cache": true
    },
    "test": {
      "options": {
        "cwd": ".",
        "command": "if [ \"$(uname -s)\" != \"Linux\" ]; then echo 'Skipping: wrong platform (need linux)'; exit 0; fi && ./epsilon --test epsilon.linux"
      },
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "{projectRoot}/**/*.lisp"
      ],
      "executor": "nx:run-commands",
      "cache": true
    }
  },
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "epsilon-linux",
  "sourceRoot": ".",
  "implicitDependencies": [
    "epsilon-foreign"
  ],
  "projectType": "library"
}
